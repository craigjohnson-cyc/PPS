@page "/clientDetails/add/"
@using DataModels.Helpers
@inject NavigationManager NavigationManager
@inject IClientDetailService ClientDetailService
@rendermode InteractiveAuto


<h3>Add Client Details</h3>

<div class="text-center">
    <h1 class="display-4">Client Details</h1>
    <table class="table">
        <thead>
            <tr>
                <th>
                    First Name
                </th>
                <th>
                    Last Name
                </th>
                <th>
                    Also Known As
                </th>
            </tr>
        </thead>
        <tbody>
            @{
                <tr>
                    <td>
                        client.fname
                    </td>
                    <td>
                        client.lname
                    </td>
                    <td>
                        client.aka
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

<label class="visually-hidden">client detail key</label>
<input class="visually-hidden" type="text" placeholder="" asp-for="clientDetail.clientDetail_key" />

<label class="visually-hidden">client key</label>
<input class="visually-hidden" type="text" placeholder="" asp-for="clientDetail.client_key" />

